# Configuración de PostgreSQL
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8npass
POSTGRES_DB=n8n

# Asegurar configuración PostgreSQL
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432

# Configuración de n8n
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=n8npass
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123
N8N_DIAGNOSTICS_ENABLED=false

# Corregir configuración n8n
N8N_PROTOCOL=http
N8N_HOST=localhost

# Configuración de Ollama
OLLAMA_HOST=http://ollama:11434
OLLAMA_API_BASE_URL=http://ollama:11434
N8N_OLLAMA_HOST=http://ollama:11434

# Configuración de Puertos
N8N_PORT=5678
OLLAMA_PORT=11434

# Configuración del Webhook y Modo de Ejecución
N8N_WEBHOOK_TUNNEL_URL=http://localhost:5678
N8N_DEFAULT_EXECUTION_MODE=queue

# Configuración de Zona Horaria
GENERIC_TIMEZONE=America/Santiago
TZ=America/Santiago

# Clave de Encriptación (nueva clave generada)
N8N_ENCRYPTION_KEY=n8n_encryption_key_$(Get-Random -Minimum 10000 -Maximum 99999)

# Cambiar protocolo a HTTP
N8N_PROTOCOL=http
N8N_WEBHOOK_TUNNEL_URL=http://localhost:5678

# Habilitar debug para más información
N8N_LOG_LEVEL=debug
